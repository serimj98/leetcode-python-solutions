class Solution:
  def moveZeroes(self, nums: List[int]) -> None:
    """
    Do not return anything, modify nums in-place instead.
    """
    #Solution 1: O(n) / O(1)
    zero_idx = 0
    for i in range(len(nums)):
      if nums[i] != 0:
        nums[i], nums[zero_idx] = nums[zero_idx], nums[i]
        zero_idx += 1 # check if next element is a zero
