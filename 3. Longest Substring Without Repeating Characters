class Solution:
    def lengthOfLongestSubstring(self, s: str) -> int:
        #Solution 1: O(n) / O(n)
        sol = 0
        seen = Counter()
        l, r = 0, 0
        while r < len(s):
            seen[s[r]] += 1
            while seen[s[r]] > 1:
                seen[s[l]] -= 1 #decrement left pointer until count == 1 for right pointer
                l += 1
            sol = max(sol, r - l + 1)
            r += 1
        return sol
