class Solution:
  def maxSubArray(self, nums: List[int]) -> int:
    #Solution 1: O(n) / O(1)
    for i in range(1, len(nums)):
      if nums[i-1] > 0:
        nums[i] += nums[i-1]
    return max(nums)
  
    #Solution 2: O(n) / O(1)
    cur = maxi = nums[0]
    for i in range(1, len(nums)):
      cur = max(nums[i], cur + nums[i])
      maxi = max(maxi, cur)
    return maxi
