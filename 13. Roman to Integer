class Solution:
  def romanToInt(self, s: str) -> int:
    #Solution 1: O(n) / O(1)
    sol = 0
    roman_dict = {'I':1, 'V':5, 'X':10, 'L':50, 'C':100, 'D':500, 'M':1000}
    for i in range(len(s)-1):
      if roman_dict[s[i]] < roman_dict[s[i+1]]:
        sol -= roman_dict[s[i]]
      else:
        sol += roman_dict[s[i]]
    return sol + roman_dict[s[-1]]
